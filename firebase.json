{
  "hosting": {
    "source": "Frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1" 
    },
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "backend-service",
          "region": "us-central1"
        }
      }
    ]
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}